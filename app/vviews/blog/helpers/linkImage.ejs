<%
    helperLinkImg = (link,alt) => {
        let linkImg = ['',''];
        let myRegex = /<img[^>]+src="(https:\/\/[^">]+)"/g;
        
        linkImg = myRegex.exec(link);

        let xhtml = ``;
        if(linkImg !== null){
            let imgSrc = '';
            imgSrc =     linkImg[1];        
             xhtml = `<img src="${imgSrc}" alt="${alt}" class="img_rss">`;
        }   
        return xhtml;
    }
%>
