<%
    helperLinkImg = (link) => {
     
        let linkImg = [];
        let myRegex = /<img[^>]+src="(https:\/\/[^">]+)"/g;
        
        linkImg = myRegex.exec(link);
        let xhtml = ``;
        if(linkImg !== null){
            let imgSrc = '';
            imgSrc =  linkImg[1];        
            xhtml = `${imgSrc}`;
        }   
        return xhtml;
    }
%>
