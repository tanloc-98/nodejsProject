<% 
    filterStatusHelper = (linkPrefix, statusFilter,  keyword) => {
        let xhtmlResult = '';
        statusFilter.forEach((item)=> {
            if(item.count == 0) return;
            let link =  linkPrefix + '/status/' + item.value;
            if(keyword !== "" && keyword !== undefined) link += '?keyword=' + keyword;

            xhtmlResult += `<a class="mr-1 btn btn-sm  btn-${item.class}" href="${link}">
                ${item.name.toUpperCase()} <span class="badge badge-pill badge-light">${item.count}</span>
            </a> `;
        })
        return xhtmlResult;
    } 
%>



